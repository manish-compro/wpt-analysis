    "webpack": "^4.41.0",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.4",
